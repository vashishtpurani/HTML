	<p>This example Buit in Object Demo </p>
		<script>
			myFunction();
			function myFunction() 
			{
				var marks = {Maths:50, English:70, Science: 71};
				var Student = {firstName:"Soham", lastName:"Patel", age:20, year:"FYBCA", marks: marks};
				

				document.write("<hr>");
				document.write("Student Name :  " + Student.firstName + " " + Student.lastName); 
				document.write("<br> Student Age : " + Student.age); 
				document.write("<br> Age of Student : " + Student.year); 
				document.write("<br> English : " + Student.marks.Maths); 
				document.write("<br> Age of Student : " + Student.year); 
				document.write("<br> Age of Student : " + Student.year); 
				document.write("<hr>");
			}
		</script>

