		document.write("<h4> value of Y = " + y + "  <h4>"); 
		document.write("<hr>");

		//x += y;
		//document.write("<h5> X + Y = " + x + "  <h5>"); 
		//x -= y;
		//document.write("<h5> X - Y = " + x + "  <h5>"); 
		x /= y;
		document.write("<h5> X * Y = " + x + "  <h5>"); 
		document.write("<hr>");
		
	</script>

